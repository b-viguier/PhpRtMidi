{
    "name": "bviguier/php-rtmidi",
    "description": "Realtime MIDI input/output, thanks to RtMidi and FFI.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "bviguier\\RtMidi\\": "src/"
        }
    },
    "autoloadev": {
        "psr-4": {
            "bviguier\\tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4",
        "ext-FFI": "^7.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.54",
        "phpunit/phpunit": "^9.4",
        "phpstan/phpstan-phpunit": "^0.12.16"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Benoit Viguier",
            "email": "benoit.viguier.pro+github@gmail.com"
        }
    ],
    "scripts": {
        "phpstan": "phpstan analyse",
        "phpunit": "phpunit tests",
        "tests": [
            "@phpstan",
            "@phpunit"
        ]
    }
}
